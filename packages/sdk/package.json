{
  "name": "@segmentationapi/sdk",
  "version": "0.6.1",
  "description": "Official JavaScript SDK for Segmentation API.",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/segmentationAPI/sdk"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "clean": "rm -rf dist coverage",
    "build:release": "tsup",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepublishOnly": "pnpm run build:release && pnpm run test && pnpm run typecheck"
  },
  "keywords": [
    "segmentation",
    "sdk",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.28.1",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "zod": "^4.1.13"
  }
}
